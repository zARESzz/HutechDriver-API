                    ]
                });

                $('.owl-carousel .nav-link').on('click', function (e) {
                    e.preventDefault();
                    const self = $(this);
                    const href = self.attr('href');
                    const target = $(href);

                    if (!self.hasClass('tab-active')) {
                        self.closest('.nav-tabs').find('.tab-active').removeClass('tab-active');
                        self.addClass('tab-active');
                        if (href && target.length) {
                            const tabContent = target.closest('.tab-content');
                            tabContent.find('.active').removeClass('active');
                            target.tab('show');
                        }
                    }
                });

                $('.owl-carousel').on('changed.owl.carousel', function (event) {

                    $('.owl-carousel').find('.owl-item').eq(event.item.index).find('.nav-item').trigger('click');
                });
            }
            const windowHref = window.location.href;
            const v_pill_index = windowHref.search('v-pills');
            if (v_pill_index > -1) {
                $(`#${windowHref.substring(v_pill_index, windowHref.length)}-tab`).trigger('click');
            }
        });
    </script>

