            const navPillActive = $('.col-sidebar .nav-link.active');
            if (navPillActive.length) {
                $('.col-sidebar .nav-control .text').html(navPillActive.text());
            }

            $('.col-sidebar .nav-control').on('click', function () {
                const self = $(this);
                const navContent = self.closest('.col-sidebar').find('.nav-pills');

                navContent.stop().slideToggle();
            });

            $('.col-sidebar .nav-link').on('click', function (e) {
                const self = $(this);
                const text = self.text();
                const navContent = self.closest('.col-sidebar').find('.nav-pills');
                const navControl = self.closest('.col-sidebar').find('.nav-control');
                const w = $(window).width();

                navControl.find('.text').html(text);
                if (w < 992) {
                    navContent.stop().slideUp();
                };
                window.location.href = location.protocol + '//' + location.host + location.pathname + self.attr('href');
            });

            $(window).on('resize', function () {
                const w = $(window).width();
                if (w > 992) {
                    $('.col-sidebar').find('.nav-pills').attr('style', '');
                }
            });

            $('.faqs__list .item__title').on('click', function () {
                const self = $(this);
                const item = self.closest('.item');
                const content = item.find('.item__content');

                item.toggleClass('active');
                content.stop().slideToggle();
            });

            if ($('.owl-carousel').length) {
                $('.owl-carousel').owlCarousel({
                    margin: 0,
                    loop: false,
                    autoWidth: true,
                    items: 4,
                    dots: false,
                    nav: true,
                    navText: [
                        '<svg class="icon" width="11" height="18" viewBox="0 0 11 18" fill="none" xmlns="http://www.w3.org/2000/svg"><path d="M1.875 16L9.125 8.75L1.875 1.5" stroke="currentColor" stroke-width="3" stroke-linecap="round"stroke-linejoin="round" /></svg>',
