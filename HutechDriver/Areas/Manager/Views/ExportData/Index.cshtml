@{
    ViewBag.Title = "Export Data";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}


<h3>Xuất danh sách đơn đặt xe</h3>
<div class="col-6">
    @using (Html.BeginForm("ExportToExcel", "Booking", FormMethod.Post))
    {
        <div class="form-group">
            <button type="submit" class="btn btn-primary"><i class="fas fa-file-csv"></i>  Export Đơn đặt xe </button>
        </div>
    }
</div>

<h3>Xuất danh sách tài khoản</h3>
@using (Html.BeginForm("ExportToExcel", "PhanQuyen", FormMethod.Post))
{

    <button type="submit" class="btn btn-primary"><i class="fas fa-file-csv"></i>  Export Danh sách tài khoản </button>

}